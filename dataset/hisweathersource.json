{
	"name": "hisweathersource",
	"properties": {
		"linkedServiceName": {
			"referenceName": "HttpServerHisW",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"lon": {
				"type": "string",
				"defaultValue": "71.0565"
			},
			"lat": {
				"type": "string",
				"defaultValue": "42.3555"
			},
			"appid": {
				"type": "string",
				"defaultValue": "85d5d295e6b5d280fb5e91fa360ea631"
			},
			"end": {
				"type": "string",
				"defaultValue": "div(sub(ticks(addDays(utcNow(), -7 * item() + 6)), ticks('1970-01-01T00:00:00Z')), 10000000)"
			},
			"start": {
				"type": "string",
				"defaultValue": "div(sub(ticks(addDays(utcNow(), -7 * item())) , ticks('1970-01-01T00:00:00Z')), 10000000)"
			},
			"dataType": {
				"type": "String",
				"defaultValue": "hourly"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": {
					"value": "data/2.5/history/city?lat=@{dataset().lat}&lon=@{dataset().lon}&type=@{dataset().dataType}&start=@{dataset().start}&end=@{dataset().end}&appid=@{dataset().appid}",
					"type": "Expression"
				}
			}
		},
		"schema": {}
	}
}